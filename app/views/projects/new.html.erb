<h1>Create Projects</h1></br>


    <%= form_for(:project, url: user_tasks_path ) do |f| %>
      <%= f.label :Name %><br>
      <%= f.text_field :name, class: 'form-control' %><br><br>
      
      <%= f.label :Startdate %><br>
      <%= f.date_field :s_date, class: 'form-control' %><br><br>
      

      <%= f.label :EndDate %><br>
      <%= f.date_field :e_date, class: 'form-control' %><br><br>

     
      <%= f.submit "Create Project", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>